  <complexType name="genericFileInstruction">
    <annotation>
      <documentation>Defines the schema for a file instruction xml file for the CDE.  In order to allow files to have pretty urls and be indexed by the search engine, there needs to be a record of that item in the file system.  This is that record.</documentation>
    </annotation>
    <all>
      <element name="MimeType" type="string" minOccurs="0" maxOccurs="1">
        <annotation>
          <documentation>Multipurpose Internet Mail Extensions</documentation>
        </annotation>
      </element>
      <element name="ContentItemInfo" type="cde:contentItemInfo" minOccurs="0" maxOccurs="1">
        <annotation>
          <documentation>The information of the content item this Assembly Item represents</documentation>
        </annotation>
      </element>
      <element name="SearchMetadata" type="cde:searchMetadata" minOccurs="0" maxOccurs="1">
        <annotation>
          <documentation>Defines metadata for this Assembly Instruction used for searching</documentation>
        </annotation>
      </element>
      <element name="PageMetadata" type="cde:pageMetadata"
        minOccurs="1" maxOccurs="1">
        <annotation>
          <documentation>
            The Metadata of the Single Page Assembly
            Instruction
          </documentation>
        </annotation>
      </element>
      <element name="ContentDates" type="cde:contentDates" minOccurs="1" maxOccurs="1">
        <annotation>
          <documentation>The dates for this File Instructions.</documentation>
        </annotation>
      </element>
      <element name="PrettyUrl" type="cde:path" minOccurs="1"
        maxOccurs="1">
        <annotation>
          <documentation>
            The primary pretty (friendly) url of the File Instruction.
          </documentation>
        </annotation>
      </element>
      <element name="FilePath" type="string" minOccurs="1" maxOccurs="1">
        <annotation>
          <documentation>
            The actual file path of the file on the web site.
          </documentation>
        </annotation>
      </element>
      <element name="SectionPath" type="cde:path" minOccurs="1"
        maxOccurs="1">
        <annotation>
          <documentation>
            The path of all of the parent folders of the
            Single Page Assembly Instruction. E.G. If the
            PrettyUrl of this Single Page Assembly Info is
            '/folder1/folder2/page1' then the SectionPath
            should be '/folder1/folder2'.

            The CDE uses this information to find the
            Section Details for the section this Single Page
            Assembly Instruction belongs to.

            While this might seem obvious, certain Single
            Page Assembly Instruction will be the default
            page for a folder (much like index.html), and
            thier PrettyUrl will be the same as the
            SectionPath.

            Furthermore, the url structure of the site may
            not map one to one with the folder structure.
          </documentation>
        </annotation>
      </element>
      <element name="Language" minOccurs="1" maxOccurs="1">
        <annotation>
          <documentation>
            Defines the ISO 639-1 code for the Single Page
            Assembly Instruction.
          </documentation>
        </annotation>
        <simpleType>
          <annotation>
            <documentation>
              One of the possible two character ISO 639-1
              language codes as defined at
              http://en.wikipedia.org/wiki/List_of_ISO_639-1_codes
            </documentation>
