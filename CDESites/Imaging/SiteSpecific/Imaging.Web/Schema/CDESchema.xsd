        <annotation>
          <documentation>
            The Metadata of the Single Page Assembly
            Instruction
          </documentation>
        </annotation>
      </element>
      <element name="ContentDates" type="cde:contentDates" minOccurs="1" maxOccurs="1">
        <annotation>
          <documentation>The dates for this MultiPage Container.</documentation>
        </annotation>
      </element>
      <element name="Snippets" minOccurs="1" maxOccurs="1">
        <annotation>
          <documentation>
            A collection of all of the snippets for the
            Single Page Assembly Instruction published from
            Percussion.
          </documentation>
        </annotation>
        <complexType>
          <annotation>
            <documentation>
              Defines a collection of one or more Snippets
              associated with this Single Page Assembly
              Instruction.
            </documentation>
          </annotation>
          <sequence>
            <element name="SnippetInfo"
              type="cde:snippetInfo" minOccurs="0" maxOccurs="unbounded">
              <annotation>
                <documentation>
                  A Snippet in the collection of
                  snippets associated with this Single
                  Page Assembly Instruction.
                </documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="Pages" minOccurs="1" maxOccurs="1">
        <annotation>
          <documentation>
            A collection of all of the snippets for the
            Single Page Assembly Instruction published from
            Percussion.
          </documentation>
        </annotation>
        <complexType>
          <annotation>
            <documentation>
              Defines a collection of one or more Snippets
              associated with this Single Page Assembly
              Instruction.
            </documentation>
          </annotation>
          <sequence>
            <element name="Page"
              type="cde:Page" minOccurs="1" maxOccurs="unbounded">
              <annotation>
                <documentation>
                  A Snippet in the collection of
                  snippets associated with this Single
                  Page Assembly Instruction.
                </documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="BlockedSlots" minOccurs="1" maxOccurs="1">
        <complexType>
          <sequence>
            <element name="Slot" minOccurs="0"
              maxOccurs="unbounded">
              <complexType>
                <attribute name="Name" type="string">
                </attribute>
              </complexType>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="LocalFields" minOccurs="0">
        <complexType>
          <sequence>
            <element name="Field" nillable="true" minOccurs="0" maxOccurs="unbounded">
              <complexType>
                <simpleContent>
                  <extension base="string">
                    <attribute name="Name" form="unqualified" type="string" />
                  </extension>
                </simpleContent>
              </complexType>
            </element>
          </sequence>
        </complexType>
      </element>

